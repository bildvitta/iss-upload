{
    "name": "bildvitta/iss-image",
    "description": "This is my package IssImage",
    "keywords": [
        "bildvitta",
        "laravel",
        "iss-image"
    ],
    "homepage": "https://github.com/bildvitta/iss-image",
    "license": "MIT",
    "authors": [
        {
            "name": "BILDjean.garcia",
            "email": "jean.garcia@bild.com.br",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.37"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Bildvitta\\IssImage\\": "src",
            "Bildvitta\\IssImage\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bildvitta\\IssImage\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bildvitta\\IssImage\\IssImageServiceProvider"
            ],
            "aliases": {
                "IssImage": "Bildvitta\\IssImage\\IssImageFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
